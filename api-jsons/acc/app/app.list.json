{
    "id":"acc.app.list",
    "name":"获取应用列表",
    "description":"列出所有应用",
    "namespace":"Kuga\\Api\\Acc",
    "method":"App.appList",
    "accessLevel": 1,
    "request":[
        {
            "param":"current",
            "required":false,
            "default":1,
            "name":"页码",
            "type":"Integer",
            "description":"页码"
        },
        {
            "param":"pageSize",
            "required":false,
            "default":10,
            "type":"Integer",
            "description":"每页显示条数",
            "name":"每页显示条数"
        }
    ],
    "response":{
        "data":{
            "type":"Object",
            "responseItem": {
                "list":{
                    "type":"Array",
                    "responseItem":{
                        "id":{
                            "type":"Integer",
                            "sample":1,
                            "description":"应用ID"
                        },
                        "name":{
                            "type":"String",
                            "sample":"WMS",
                            "description":"应用名称"
                        },
                        "secret":{
                            "type":"String",
                            "sample":"SX9dfsafda0",
                            "description":"应用Secret"
                        },
                        "disabled":{
                            "type":"Integer",
                            "sample":"1",
                            "description":"是否禁用，1禁用，0启用"
                        },
                        "shortDesc":{
                            "type":"String",
                            "sample":"",
                            "description":"应用描述"
                        }
                    },
                    "description":"数据列表"
                },
                "total":{
                    "type":"Integer",
                    "description":"记录总数",
                    "sample":10
                },
                "page":{
                    "type":"Integer",
                    "description":"当前页码",
                    "sample":1
                },
                "limit":{
                    "type":"Integer",
                    "description":"每页条数",
                    "sample":10
                }
            }
        }
    }
}