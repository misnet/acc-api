{
    "id":"acc.user.changepwd.byverifycode",
    "name":"用验证码修改密码",
    "description":"修改当前用户的密码",
    "namespace":"Kuga\\Api\\Acc",
    "method":"User.changePasswordByVerifyCode",
    "accessLevel": 1,
    "request":[
        {
            "param":"receive",
            "required":true,
            "default":"",
            "type":"String",
            "description":"手机号或邮箱"
        },
        {
            "param":"verifyCode",
            "required":true,
            "default":"",
            "type":"String",
            "description":"验证码"
        },
        {
            "param":"verifyCodeToken",
            "required":true,
            "default":"",
            "type":"String",
            "description":"验证码发送后拿到的seed"
        },
        {
            "param":"password",
            "required":true,
            "default":"",
            "type":"String",
            "description":"新密码"
        }
    ],
    "response":{
        "data":{
            "type":"Boolean",
            "sample":true,
            "description":"成功返回true，失败返回false"
        }
    }
}
